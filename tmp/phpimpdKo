/**
 * All the different object classes used in populators
 */

namespace pocketmine\level\generator\object;


abstract class Object {

}<?php

/*
 *
 *  ____            _        _   __  __ _                  __  __ ____  
 * |  _ \ ___   ___| | _____| |_|  \/  (_)_ __   ___      |  \/  |  _ \ 
