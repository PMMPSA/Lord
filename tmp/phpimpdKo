use pocketmine\block\Block;

abstract class GrassyBiome extends NormalBiome {

	/**
	 * GrassyBiome constructor.
	 */
	public function __construct(){
		$this->setGroundCover([
			Block::get(Block::GRASS, 0),
			Block::get(Block::DIRT, 0),
			Block::get(Block::DIRT, 0),
			Block::get(Block::DIRT, 0),
			Block::get(Block::DIRT, 0),
		]);
	}
}<?php

