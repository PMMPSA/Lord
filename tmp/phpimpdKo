namespace pocketmine\block;


abstract class Transparent extends Block {

	/**
	 * @return bool
	 */
	public function isTransparent(){
		return true;
	}
}<?php

/*
 *
 *  ____            _        _   __  __ _                  __  __ ____  
 * |  _ \ ___   ___| | _____| |_|  \/  (_)_ __   ___      |  \/  |  _ \ 
